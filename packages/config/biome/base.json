{
	"$schema": "https://biomejs.dev/schemas/2.2.4/schema.json",
	"formatter": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"suspicious": {
				"noUnknownAtRules": "off",
				"noArrayIndexKey": "off"
			},
			"correctness": {
				"noChildrenProp": "off",
				"useExhaustiveDependencies": "off"
			}
		}
	},
	"json": {
		"formatter": {
			"enabled": true
		}
	}
}
