{
	"name": "@ecehive/server",
	"version": "2.0.2",
	"type": "module",
	"scripts": {
		"start": "bun ./src/index.ts",
		"dev": "bun --watch ./src/index.ts",
		"build": "bun build --outdir ./dist --target bun ./src/index.ts",
		"check": "biome check --write",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@ecehive/env": "workspace:*",
		"@ecehive/features": "workspace:*",
		"@ecehive/logger": "workspace:*",
		"@ecehive/rest": "workspace:*",
		"@ecehive/trpc": "workspace:*",
		"@ecehive/workers": "workspace:*",
		"@fastify/cors": "^11.2.0",
		"@trpc/server": "11.7.2",
		"fastify": "^5.6.2"
	},
	"devDependencies": {
		"@ecehive/config": "workspace:*",
		"typescript": "^5.9.3"
	}
}
