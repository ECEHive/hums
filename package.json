{
	"name": "@ecehive/hums",
	"version": "1.5.0",
	"type": "module",
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"scripts": {
		"dev": "bun --filter './apps/*' dev",
		"check": "biome check --write",
		"typecheck": "bun --filter './packages/*' --filter './apps/*' typecheck",
		"set-version": "bun scripts/set-package-versions.ts"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.8"
	}
}
